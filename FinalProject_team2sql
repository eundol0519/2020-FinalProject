-- 회원 정보 테이블
CREATE TABLE MEMBERS(
    USERID NVARCHAR2(15) PRIMARY KEY,
    USERPW NVARCHAR2(100) NOT NULL,
    USERNAME NVARCHAR2(5) NOT NULL,
    USERIMAGE NVARCHAR2(100),
    USERBIRTH DATE,
    USERADDRESS NVARCHAR2(50),
    USERGENDER NVARCHAR2(2) NOT NULL,
    USEREMAIL NVARCHAR2(50) NOT NULL,
    USERPHONE NVARCHAR2(11) NOT NULL UNIQUE,
    USERCONFIRM NVARCHAR2(2),
    USERLOCK DATE,
    USERLOCKCOUNT NUMBER default 0,
    USERPOINT NUMBER default 0,
    NAVERID NVARCHAR2(20) UNIQUE,
    KAKAOID NVARCHAR2(20) UNIQUE,
    FACEBOOKID NVARCHAR2(20) UNIQUE
);
CREATE VIEW MEMBERLIST AS SELECT B.*, ROW_NUMBER() OVER(ORDER BY USERID) AS RN FROM MEMBERS B;
CREATE VIEW SUSPENDLIST AS SELECT B.*, ROW_NUMBER() OVER(ORDER BY USERID) AS RN FROM MEMBERS B WHERE USERLOCK>SYSDATE;


select*from members;
commit;
delete members where useremail='eundol0519@naver.com';


-- 쿠폰 정보 테이블
CREATE TABLE COUPON(
    COUPONNAME NVARCHAR2(50) NOT NULL, 
    COUPONCODE NVARCHAR2(12) PRIMARY KEY,
    COUPONDATE DATE NOT NULL,
    COUPONCONTENTS NVARCHAR2(50) NOT NULL,
    COUPONSALE NUMBER NOT NULL
);
INSERT INTO COUPON VALUES('50퍼할인쿠폰','ddaa',SYSDATE+100,'50퍼센트 할인쿠폰',50);
-- 회원이 가진 쿠폰 테이블
CREATE TABLE MEMBER_COUPON(
    USERID NVARCHAR2(15),
    COUPONCODE NVARCHAR2(12),

    CONSTRAINT MB_CP_PK PRIMARY KEY(USERID, COUPONCODE),
    CONSTRAINT MC_UI_FK FOREIGN KEY (USERID) REFERENCES MEMBERS(USERID) ON DELETE CASCADE,
    CONSTRAINT MC_CC_FK FOREIGN KEY (COUPONCODE) REFERENCES COUPON(COUPONCODE) ON DELETE CASCADE
);




