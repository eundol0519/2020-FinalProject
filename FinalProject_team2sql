CREATE TABLE HOTEL(
    HOTELSIZE NVARCHAR2(2) NOT NULL,
    HOTELCODE NVARCHAR2(4) PRIMARY KEY,
    HOTELROOMNAME NVARCHAR2(10) NOT NULL,
    HOTELPRICE NUMBER NOT NULL,
    HOTELPOINT NUMBER,
    HOTELMAXANIMAL NUMBER NOT NULL,
    HOTELCONTENTS NVARCHAR2(1000),
    HOTELROOMIMAGE1 NVARCHAR2(100),
    HOTELROOMIMAGE2 NVARCHAR2(100),
    HOTELROOMIMAGE3 NVARCHAR2(100),
    HOTELROOMIMAGE4 NVARCHAR2(100),
    HOTELROOMIMAGE5 NVARCHAR2(100),
    KIND NVARCHAR2(3) NOT NULL
);
