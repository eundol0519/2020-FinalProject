-- 병원 예약 정보 테이블
CREATE TABLE MEDICAL_RESERVATION(
    MEDICALRESERVENUM NUMBER,
    MEDICALRESERVEDATE DATE,
    MEDICALRESERVETIME NVARCHAR2(4),
    MEDICALRESERVEANIMALKIND NVARCHAR2(3) NOT NULL,
    MEDICALRESERVEANIMALAGE NUMBER,
    MEDICALRESERVEANIMALNOTE NVARCHAR2(500),
    MEDICALRESERVESUBJECT NVARCHAR2(1000) NOT NULL,
    MEDICALRESERVEDOCTOR NVARCHAR2(4) NOT NULL,
    MEDICALRESERVEUSERID NVARCHAR2(15) NOT NULL,
    MEDICALRESERVEPRICE NUMBER NOT NULL,
    MEDICALRESERVEPOINT NUMBER,
    MEDICALRESERVEPAYMENT NVARCHAR2(6) NOT NULL,
    MEDICALREVIEW NUMBER,

    CONSTRAINT MRD_MRT_PK PRIMARY KEY(MEDICALRESERVEDATE, MEDICALRESERVETIME),
    CONSTRAINT MR_RD_FK FOREIGN KEY (MEDICALRESERVEDOCTOR) REFERENCES MEDICAL_DOCTOR(DOCTORCODE) ON DELETE CASCADE,
    CONSTRAINT MR_RU_FK FOREIGN KEY (MEDICALRESERVEUSERID) REFERENCES MEMBERS(USERID) ON DELETE CASCADE
);
-- 병원 예약 뷰
CREATE VIEW MEDICAL_RESERVE_LIST AS SELECT B.*, Q.*, ROW_NUMBER() OVER(ORDER BY B.MEDICALRESERVEDATE ASC, B.MEDICALRESERVETIME ASC) 
AS RN FROM MEDICAL_RESERVATION B, MEDICAL_DOCTOR Q WHERE Q.DOCTORCODE=B.MEDICALRESERVEDOCTOR;
-- 병원 예약 시퀀스
CREATE SEQUENCE MEDICAL_RESERVE_SEQ INCREMENT BY 1 START WITH 1 ;







