    DOCTOROFF NVARCHAR2(20),
    DOCTORCAREER NVARCHAR2(1000),
    DOCTORSUBJECT NVARCHAR2(1000) NOT NULL,
    KIND NVARCHAR2(3) NOT NULL
);
