-- 용품 장바구니 정보 테이블
CREATE TABLE GOODS_BASKET(
    BASKETGOODSNUM NUMBER,
    BASKETUSERID NVARCHAR2(15),
    BASKETGOODSCOUNT NUMBER,

    CONSTRAINT GBN_GBU_PK PRIMARY KEY(BASKETGOODSNUM, BASKETUSERID),
    CONSTRAINT GB_GN_FK FOREIGN KEY (BASKETGOODSNUM) REFERENCES GOODS(GOODSNUM) ON DELETE CASCADE,
    CONSTRAINT GB_UI_FK FOREIGN KEY (BASKETUSERID) REFERENCES MEMBERS(USERID) ON DELETE CASCADE
);


-- 용품 문의 테이블
CREATE TABLE GOODS_ASK(
    ASKGOODSNUM NUMBER,
    ASKNUM NUMBER PRIMARY KEY,
    ASKCONTENTS NVARCHAR2(500) NOT NULL,
    ASKUSERID NVARCHAR2(15),
    ASKSECRET NVARCHAR2(3) NOT NULL,
    ASKPW NUMBER,
    ASKANSWER NVARCHAR2(500),

    CONSTRAINT GA_GN_FK FOREIGN KEY (ASKGOODSNUM) REFERENCES GOODS(GOODSNUM) ON DELETE CASCADE,
    CONSTRAINT GA_UI_FK FOREIGN KEY (ASKUSERID) REFERENCES MEMBERS(USERID) ON DELETE CASCADE
);
-- 용품 문의 뷰
CREATE VIEW ASK_LIST AS SELECT GD.*,B.*, ROW_NUMBER() OVER(ORDER BY ASKNUM) AS RN FROM GOODS_ASK B, GOODS GD 
WHERE ASKANSWER IS NULL AND GD.GOODSNUM=B.ASKGOODSNUM ;
-- 용품 문의 시퀀스
CREATE SEQUENCE GOODS_ASK_SEQ INCREMENT BY 1 START WITH 1 ; 


-- 용품 구매 테이블
CREATE TABLE GOODS_BUY(
    GOODSBUYNUM NUMBER,
    BUYGOODSNUM NUMBER NOT NULL,
    BUYUSERID NVARCHAR2(15) NOT NULL,
    BUYPAYMENT NVARCHAR2(6) NOT NULL,
    BUYADDRESS NVARCHAR2(50) NOT NULL,
    BUYDELEVERY NVARCHAR2(10) NOT NULL,
    BUYCOUNT NUMBER NOT NULL,
    BUYPRICE NUMBER NOT NULL,
    GOODSREVIEW NUMBER,

    CONSTRAINT GBU_GN_FK FOREIGN KEY (BUYGOODSNUM) REFERENCES GOODS(GOODSNUM) ON DELETE CASCADE,
    CONSTRAINT GBU_UI_FK FOREIGN KEY (BUYUSERID) REFERENCES MEMBERS(USERID) ON DELETE CASCADE
);
-- 용품 구매 뷰
CREATE VIEW GOODS_BUY_LIST AS SELECT B.*, Q.*, ROW_NUMBER() OVER(ORDER BY BUYGOODSNUM) 
AS RN FROM GOODS_BUY B, GOODS Q WHERE Q.GOODSNUM=B.BUYGOODSNUM;
-- 용품 구매 시퀀스
CREATE SEQUENCE GOODS_BUY_SEQ INCREMENT BY 1 START WITH 1 ;


select * from HOTEL_RESERVATION;

select * from members;
delete from members where userid='eundol0519';
commit;

-- 용품 좋아요
CREATE TABLE GOODS_LIKE(
    LIKEGOODSNUM NUMBER,
    LIKEUSERID NVARCHAR2(15),
    
    CONSTRAINT LGN_LUI_PK PRIMARY KEY(LIKEGOODSNUM, LIKEUSERID),
    CONSTRAINT GL_GN_FK FOREIGN KEY (LIKEGOODSNUM) REFERENCES GOODS(GOODSNUM) ON DELETE CASCADE,
    CONSTRAINT GL_UI_FK FOREIGN KEY (LIKEUSERID) REFERENCES MEMBERS(USERID) ON DELETE CASCADE
);




--/////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////




-- 교육 정보 테이블
CREATE TABLE EDUCATION(
    EDUTITLE NVARCHAR2(50) NOT NULL,
    EDUCONTENTS NVARCHAR2(500),
    EDUIMAGE NVARCHAR2(100) NOT NULL,
    EDUVIDEO NVARCHAR2(500) NOT NULL,
    EDULIKE NUMBER DEFAULT 0,
    EDUHIT NUMBER DEFAULT 0,
    EDUNUM NUMBER PRIMARY KEY,
    KIND NVARCHAR2(3),
    EDUANIMALKIND NVARCHAR2(10)
);
-- 교육 시퀀스
CREATE SEQUENCE EDU_SEQ INCREMENT BY 1 START WITH 1 ;
-- 교육 뷰
SELECT * FROM (SELECT B.*, ROW_NUMBER() OVER(ORDER BY EDUNUM) AS RN FROM EDUCATION B WHERE EDUANIMALKIND LIKE '%강아지%') WHERE RN BETWEEN 1 AND 3;
-- 교육 내용
