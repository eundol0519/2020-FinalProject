-- 교육 좋아요 테이블
CREATE TABLE EDUCATION_LIKE(
    LIKEEDUNUM NUMBER,
    LIKEUSERID NVARCHAR2(15),

    CONSTRAINT EEN_EUI_PK PRIMARY KEY(LIKEEDUNUM, LIKEUSERID),
    CONSTRAINT EL_EN_FK FOREIGN KEY (LIKEEDUNUM) REFERENCES EDUCATION(EDUNUM) ON DELETE CASCADE,
    CONSTRAINT EL_UI_FK FOREIGN KEY (LIKEUSERID) REFERENCES MEMBERS(USERID) ON DELETE CASCADE
);






-- 교육 댓글 정보 테이블
CREATE TABLE EDUCATION_COMMENT(
    COMMENTEDUNUM NUMBER,
    COMMENTNUM NUMBER PRIMARY KEY,
    COMMENTUSERID NVARCHAR2(15),
    COMMENTLIKE NUMBER DEFAULT 0,
    COMMENTDATE DATE NOT NULL,
    COMMENTCONTENTS NVARCHAR2(500),

    CONSTRAINT EC_EN_FK FOREIGN KEY (COMMENTEDUNUM) REFERENCES EDUCATION(EDUNUM) ON DELETE CASCADE,
    CONSTRAINT EC_UI_FK FOREIGN KEY (COMMENTUSERID) REFERENCES MEMBERS(USERID) ON DELETE CASCADE
);
--교육 댓글 시퀀스
CREATE SEQUENCE EDUCATION_COMMENT_SEQ INCREMENT BY 1 START WITH 1 ;
--교육 정보
INSERT INTO EDUCATION_COMMENT VALUES(1,EDUCATION_COMMENT_SEQ.nextVal,'admin',0,SYSDATE,'이벤트정보입니다.');




-- 교육 댓글 좋아요 테이블
CREATE TABLE EDUCATION_COMMENT_LIKE(
    LIKECOMMENTEDUNUM NUMBER,
    LIKECOMMENTNUM NUMBER,
    LIKEUSERID NVARCHAR2(15),

    CONSTRAINT LEN_LCN_LUI_PK PRIMARY KEY(LIKECOMMENTEDUNUM, LIKECOMMENTNUM, LIKEUSERID),
    CONSTRAINT ECL_CN_FK FOREIGN KEY (LIKECOMMENTEDUNUM) REFERENCES EDUCATION(EDUNUM) ON DELETE CASCADE,
    CONSTRAINT ECL_UI_FK FOREIGN KEY (LIKEUSERID) REFERENCES MEMBERS(USERID) ON DELETE CASCADE
);





--/////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////





-- 이벤트 정보 테이블
CREATE TABLE EVENT(
    EVENTNUM NUMBER PRIMARY KEY,
    EVENTTITLEIMAGE NVARCHAR2(100) NOT NULL,
    EVENTIMAGE NVARCHAR2(100),
    EVENTHIT NUMBER DEFAULT 0,
    EVENTDATE DATE NOT NULL,
    EVENTCONTENTS NVARCHAR2(1000),
    EVENTCOUPONCODE NVARCHAR2(12),
    KIND NVARCHAR2(3)
);
--이벤트 시퀀스
CREATE SEQUENCE EVENT_SEQ INCREMENT BY 1 START WITH 1 ;
--이벤트 정보
