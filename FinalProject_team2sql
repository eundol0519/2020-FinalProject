-- 이벤트 댓글 정보 테이블
CREATE TABLE EVENT_COMMENT(
    COMMENTEVENTNUM NUMBER,
    COMMENTNUM NUMBER PRIMARY KEY,
    COMMENTCONTENTS NVARCHAR2(500) NOT NULL,
    COMMENTUSERID NVARCHAR2(15),
    COMMENTIMAGE NVARCHAR2(100),
    COMMENTLIKE NUMBER DEFAULT 0,
    COMMENTDATE DATE NOT NULL,

    CONSTRAINT EVC_EN_FK FOREIGN KEY (COMMENTEVENTNUM) REFERENCES EVENT(EVENTNUM) ON DELETE CASCADE,
    CONSTRAINT EVC_UI_FK FOREIGN KEY (COMMENTUSERID) REFERENCES MEMBERS(USERID) ON DELETE CASCADE
);
--이벤트 댓글 시퀀스
CREATE SEQUENCE EVENT_COMMENT_SEQ INCREMENT BY 1 START WITH 1 ;
INSERT INTO EVENT_COMMENT VALUES(1, EVENT_COMMENT_SEQ.NEXTVAL, '우왕 댓글 달 수 이ㅏㅆ따','admin', '', 0, SYSDATE);


-- 이벤트 댓글 좋아요 테이블
CREATE TABLE EVENT_COMMENT_LIKE(
    LIKECOMMENTEVENTNUM NUMBER,
    LIKECOMMENTNUM NUMBER,
    LIKEUSERID NVARCHAR2(15),

    CONSTRAINT EVLEN_EVLCN_EVLUI_PK PRIMARY KEY(LIKECOMMENTEVENTNUM, LIKECOMMENTNUM, LIKEUSERID),
    CONSTRAINT EVCL_EN_FK FOREIGN KEY (LIKECOMMENTEVENTNUM) REFERENCES EVENT(EVENTNUM) ON DELETE CASCADE,
    CONSTRAINT EVCL_CN_FK FOREIGN KEY (LIKECOMMENTNUM) REFERENCES EVENT_COMMENT(COMMENTNUM) ON DELETE CASCADE,
    CONSTRAINT EVCL_UI_FK FOREIGN KEY (LIKEUSERID) REFERENCES MEMBERS(USERID) ON DELETE CASCADE
);




--/////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////





-- 리뷰 정보 테이블
CREATE TABLE REVIEW(
    REVIEWNUM NUMBER PRIMARY KEY,
    REVIEWCONTENTS NVARCHAR2(1000) NOT NULL,
    REVIEWIMAGE NVARCHAR2(100),
    REVIEWRATING NVARCHAR2(10) NOT NULL,
    REVIEWDATE DATE NOT NULL,
    REVIEWBLIND NVARCHAR2(5),
    REVIEWLIKE NUMBER NOT NULL,
    REVIEWTITLE NVARCHAR2(55) NOT NULL,
    REVIEWKIND NVARCHAR2(2) NOT NULL,
    REVIEWUSERID NVARCHAR2(15),

    CONSTRAINT RV_UI_FK FOREIGN KEY (REVIEWUSERID) REFERENCES MEMBERS(USERID) ON DELETE CASCADE
);

-- 리뷰 시퀀스
CREATE SEQUENCE REVIEW_SEQ INCREMENT BY 1 START WITH 1 ;
INSERT INTO REVIEW VALUES(REVIEW_SEQ.NEXTVAL, '리뷰 용용 admin 남깁니다', '201019_143017ㅡavatar1.jpg', '5', SYSDATE, '', 0, '(리뷰)용품검색', '용품', 'a');
-- 리뷰 뷰
CREATE VIEW HOTEL_REVIEW AS SELECT B.*, ROW_NUMBER() OVER(ORDER BY REVIEWDATE DESC) AS RN FROM REVIEW B WHERE REVIEWKIND='호텔';
CREATE VIEW BEAUTY_REVIEW AS SELECT B.*, ROW_NUMBER() OVER(ORDER BY REVIEWDATE DESC) AS RN FROM REVIEW B WHERE REVIEWKIND='미용';
CREATE VIEW MEDICAL_REVIEW AS SELECT B.*, ROW_NUMBER() OVER(ORDER BY REVIEWDATE DESC) AS RN FROM REVIEW B WHERE REVIEWKIND='병원';
CREATE VIEW GOODS_REVIEW AS SELECT B.*, ROW_NUMBER() OVER(ORDER BY REVIEWDATE DESC) AS RN FROM REVIEW B WHERE REVIEWKIND='용품';





-- 리뷰 좋아요 테이블
CREATE TABLE REVIEW_LIKE(
    LIKEREVIEWNUM NUMBER,
    LIKEUSERID NVARCHAR2(15),

    CONSTRAINT LRN_LUI_PK PRIMARY KEY (LIKEREVIEWNUM, LIKEUSERID),
    CONSTRAINT RL_RN_FK FOREIGN KEY (LIKEREVIEWNUM) REFERENCES REVIEW(REVIEWNUM) ON DELETE CASCADE,
    CONSTRAINT RL_UI_FK FOREIGN KEY (LIKEUSERID) REFERENCES MEMBERS(USERID) ON DELETE CASCADE
);



-- 리뷰 신고
CREATE TABLE REVIEW_REPORT(
    REPORTREVIEWNUM NUMBER,
    REPORTUSERID NVARCHAR2(15),
    REPORTKIND NVARCHAR2(1000),

    CONSTRAINT RRN_RUI_PK PRIMARY KEY (REPORTREVIEWNUM, REPORTUSERID),
    CONSTRAINT RR_RN_FK FOREIGN KEY (REPORTREVIEWNUM) REFERENCES REVIEW(REVIEWNUM) ON DELETE CASCADE,
    CONSTRAINT RR_UI_FK FOREIGN KEY (REPORTUSERID) REFERENCES MEMBERS(USERID) ON DELETE CASCADE
);
-- 리뷰 신고 뷰
CREATE VIEW REPORT_LIST AS SELECT B.*, ROW_NUMBER() OVER(ORDER BY REPORTREVIEWNUM) AS RN FROM REVIEW_REPORT B;

select*from medical_reservation;
