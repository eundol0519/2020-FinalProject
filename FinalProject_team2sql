-- 호텔 예약 정보 테이블
CREATE TABLE HOTEL_RESERVATION(
    HOTELRESERVENUM NUMBER,
    HOTELRESERVECODE NVARCHAR2(4),
    HOTELUSERNAME NVARCHAR2(5) NOT NULL,
    HOTELUSERPHONE NVARCHAR2(11) NOT NULL,
    HOTELCHECKIN DATE,
    HOTELCHECKOUT DATE,
    HOTELANIMALKIND NVARCHAR2(3) NOT NULL,
    HOTELSPECIALNOTE NVARCHAR2(1000),
    HOTELPAYMENT NVARCHAR2(6) NOT NULL,
    HOTELUSERID NVARCHAR2(15) NOT NULL,
    HOTELRESERVEPRICE NUMBER,
    HOTELRESERVEPOINT NUMBER,
    HOTELREVIEW NUMBER,

    CONSTRAINT HT_CD_FK FOREIGN KEY (HOTELRESERVECODE) REFERENCES HOTEL(HOTELCODE) ON DELETE CASCADE,
    CONSTRAINT HT_UI_FK FOREIGN KEY (HOTELUSERID) REFERENCES  MEMBERS(USERID) ON DELETE CASCADE
);
-- 호텔 예약 뷰
CREATE VIEW HOTEL_RESERVE_LIST AS SELECT B.*, Q.*, ROW_NUMBER() OVER(ORDER BY B.HOTELCHECKIN) AS RN FROM HOTEL_RESERVATION B, HOTEL Q WHERE HOTELCODE=HOTELRESERVECODE;
-- 호텔 예약 시퀀스
CREATE SEQUENCE HOTEL_RESERVE_SEQ INCREMENT BY 1 START WITH 1 ;





