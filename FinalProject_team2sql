-- 미용 예약 테이블
CREATE TABLE BEAUTY_RESERVATION(
    BEAUTYRESERVENUM NUMBER,
    BEAUTYRESERVEDATE DATE,
    BEAUTYRESERVETIME NVARCHAR2(4),
    BEAUTYRESERVEANIMALKIND NVARCHAR2(3) NOT NULL,
    BEAUTYRESERVEANIMALAGE NUMBER,
    BEAUTYRESERVEANIMALNOTE NVARCHAR2(1000),
    BEAUTYRESERVESUBJECT NVARCHAR2(50) NOT NULL,
    BEAUTYRESERVEDESIGNER NVARCHAR2(4) NOT NULL,
    BEAUTYRESERVEUSERID NVARCHAR2(15) NOT NULL,
    BEAUTYRESERVEPRICE NUMBER NOT NULL,
    BEAUTYRESERVEPOINT NUMBER,
    BEAUTYRESERVEPAYMENT NVARCHAR2(6),
    BEAUTYREVIEW NUMBER,

    CONSTRAINT BRD_BRT_PK PRIMARY KEY(BEAUTYRESERVEDATE, BEAUTYRESERVETIME),
    CONSTRAINT BR_RD_FK FOREIGN KEY (BEAUTYRESERVEDESIGNER) REFERENCES BEAUTY_DESIGNER(DESIGNERCODE) ON DELETE CASCADE,
    CONSTRAINT BR_UI_FK FOREIGN KEY (BEAUTYRESERVEUSERID) REFERENCES MEMBERS(USERID) ON DELETE CASCADE
);
-- 미용 예약 뷰
CREATE VIEW BEAUTY_RESERVE_LIST AS SELECT B.*, Q.*, ROW_NUMBER() OVER(ORDER BY B.BEAUTYRESERVEDATE ASC, B.BEAUTYRESERVETIME ASC) 
AS RN FROM BEAUTY_RESERVATION B, BEAUTY_DESIGNER Q WHERE Q.DESIGNERCODE=B.BEAUTYRESERVEDESIGNER;
-- 미용 예약 시퀀스
CREATE SEQUENCE BEAUTY_RESERVE_SEQ INCREMENT BY 1 START WITH 1 ;



--/////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////



-- 용품 정보 테이블
CREATE TABLE GOODS(
    GOODSNUM NUMBER PRIMARY KEY,
    GOODSNAME NVARCHAR2(50) NOT NULL,
    GOODSPRICE NUMBER NOT NULL,
    GOODSSTOCK NUMBER NOT NULL,
    GOODSANIMALKIND NVARCHAR2(10) NOT NULL,
    GOODSCATEGORY NVARCHAR2(10) NOT NULL,
    GOODSLIKE NUMBER NOT NULL,
    GOODSHIT NUMBER NOT NULL,
    GOODSCONTENTS NVARCHAR2(500) NOT NULL,
    GOODSIMAGE1 NVARCHAR2(100),
    GOODSIMAGE2 NVARCHAR2(100),
    GOODSIMAGE3 NVARCHAR2(100),
    GOODSIMAGE4 NVARCHAR2(100),
    GOODSIMAGE5 NVARCHAR2(100),
    GOODSPOINT NUMBER,
    KIND NVARCHAR2(2)
);
--용품 시퀀스
CREATE SEQUENCE GOODSNUM_SEQ INCREMENT BY 1 START WITH 1 ; 
--용품 뷰
CREATE VIEW GOODS_LIST AS SELECT B.*, ROW_NUMBER() OVER(ORDER BY GOODSNUM) AS RN FROM GOODS B;
